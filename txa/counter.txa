#[

define $count-down
    local @n
:loop
    @n print
    @n 1 sub
    local @n
    @n 0 lst jump :end
    1 jump :loop
:end
endef

define $main
    6 $count-down
    -3 $count-down
endef

]#

define $call-1
1 sub
    local @n
    @n 0 lst jump :end
    "call-1" print
    @n print
    @n $call-2
:end
endef

define $call-2
    local @c
    @c 0 lst jump :end
    "call-2" print
    @c print
    @c $call-1
:end
endef

define $main
    6 $call-1
endef